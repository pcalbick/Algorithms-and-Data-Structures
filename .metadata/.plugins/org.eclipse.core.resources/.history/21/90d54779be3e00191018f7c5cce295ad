package Sorts;

import java.util.Comparator;

public class File {
	
	private static Comparator<File> ALPHABETICAL = new alphabetcial();
	private final String s;
	
	public File(String s) {
		this.s = s;
	}
	
	private static class alphabetcial implements Comparator<File> {
		public int compare(File p, File q) {
			return p.compareTo(q);
		}
	}
	
	public int compareTo(File that) {
		for(int i=0; i<this.s.length(); i++) {
			if(i == that.s.length() || this.s.charAt(i) > that.s.charAt(i)) return 1;
			else if((i == this.s.length()-1 && i < that.s.length()-1) || this.s.charAt(i) < that.s.charAt(i)) return -1;
		}
		return 0;
	}
	
	public String get() {
		return s;
	}
}
