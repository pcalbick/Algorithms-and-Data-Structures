package UnionFind;

public class GenericUnionFind {
	
	private int[] arr;
	private int[] id;
	private int[] size;
	
	public GenericUnionFind(int[] arr) {
		this.arr = arr;
		id = new int[arr.length];
		size = new int[arr.length];
		for(int i=0; i<arr.length; i++) {
			id[i] = i;
			size[i] = 1;
		}
	}
	
	public 
}
