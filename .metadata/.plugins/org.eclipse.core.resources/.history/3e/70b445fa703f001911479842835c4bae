package Sorts;

import java.util.Comparator;

public class QuickSort {
	
	private InsertionSort insertionSort;
	private Shuffle shuffle;
	
	public QuickSort() {
		insertionSort = new InsertionSort();
		shuffle = new Shuffle();
	}
	
	public void sort(File[] arr, Comparator<File> comparator) {
		shuffle.shuffle(arr);
	}
}
