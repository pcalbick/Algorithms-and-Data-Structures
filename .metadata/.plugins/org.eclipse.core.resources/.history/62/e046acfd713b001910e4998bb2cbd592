package Bags;

public class BagArray implements Iterable<Item> {
	
	private int n = 0;
	private Item[] array;
	
	public BagArray() {
		array = (Item[]) new Object[1];
	}
	
	public void add(Item item) {
		array[n++] = item;
		if(n > array.length/2) array = resize(array.length*2);
	}
	
	public Item get(Item item) {
		return null;
	}
}
