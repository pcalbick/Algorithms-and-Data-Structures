package Searches;

public class BinarySearch {
	
	public int find(int i, int[] args) {
		int low = 0;
		int high = args.length-1;
		while(low != i) {
			int middle = ((high-low)/2)+low;
			if(args[middle] > i) high = middle;
			else if(args[middle] < i) low = middle;
			else return -1;
		}
		return low;
	}
}
