package Searches;

import java.util.Scanner;

public class BinarySearch {
	
	public int find(int i, int[] args) {
		int low = 0;
		int high = args.length-1;
		while(low != i) {
			int middle = ((high-low)/2)+low;
			if(args[middle] > i) high = middle;
			else if(args[middle] < i) low = middle;
			else return -1;
		}
		return low;
	}
	
	public static void main(String[] args) {
		int[] array = new int[10];
		Scanner scanner = new Scanner;
		int count = 0;
		while(!scanner.nextLine().equals(".") && count < array.length) {
			array[count] = Integer.parseInt((scanner.nextLine());
		}
	}
}
