package Stack;

import Stack.Item;

public class StackLinkedList<Item> {
	
	private Item first = null;
	
	public void push(String s) {
		Item old = first;
		first = new Item();
		first.s = s;
		first.next = old;
	}
	
	public String pop() {
		String s = first.s;
		first = first.next;
		return s;
	}
	
	public boolean isEmpty() {
		return first == null;
	}
}
